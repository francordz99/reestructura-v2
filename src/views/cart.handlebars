<div>
  <h1>Carrito</h1>
  <table>
    <thead>
      <tr>
        <th>Producto</th>
        <th>Cantidad</th>
      </tr>
    </thead>
    <tbody>
      {{#each cart.products}}
        <tr data-id={{this.product._id}}>
          <td>{{this.product}}</td>
          <td>{{this.quantity}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>