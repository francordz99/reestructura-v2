<div class="container mt-5">
  <h1 class="mb-4">Carrito</h1>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Producto</th>
        <th scope="col">Cantidad</th>
      </tr>
    </thead>
    <tbody>
      {{#each cart.products}}
      <tr data-id={{this.product._id}}>
        <td>{{this.product}}</td>
        <td>{{this.quantity}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>